{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "eleventy.js",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "crdeps": "npm i swup @swup/head-plugin nunjucks && npm i @11ty/eleventy concurrently esbuild --save-dev",
    "cleandeps": "rm -rf node_modules package-lock.json",
    "eldev": "sleep 1 && eleventy --serve",
    "elprod": "eleventy",
    "esbstart": "esbuild src/js/script.js --bundle --outfile=public/js/bundle.js --sourcemap",
    "esbdev": "esbuild src/js/script.js --bundle --outfile=public/js/bundle.js --watch --sourcemap",
    "esbprod": "esbuild src/js/script.js --bundle --outfile=public/js/bundle.js --minify",
    "esbadmincms": "esbuild src/js/admin.js --bundle --outfile=public/js/admin-bundle.js --minify",
    "endprod": "npm run esbprod && npm run elprod",
    "endprodcms": "npm run esbprod && npm run esbadmincms && npm run elprod",
    "start": "concurrently \"npm:esbdev\" \"npm:eldev\""
  },
  "dependencies": {
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.3"
  }
}
